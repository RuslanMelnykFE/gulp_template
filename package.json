{
  "name": "gulp-template",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "gulp dev",
    "prod": "gulp prod --production && gulp serve",
    "prod:views": "gulp views --production",
    "prod:styles": "gulp styles --production",
    "prod:scripts": "gulp scripts --production",
    "prod:images": "gulp images --production",
